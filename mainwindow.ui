QMainWindow {
    background-color: #ECEFF1;
}

QWidget {
    color: #212121;
    font-size: 10pt;
}

QTabWidget::pane {
    border: 1px solid #BDBDBD;
    background-color: #FFFFFF;
    margin-top: -1px;
    border-radius: 0 0 4px 4px;
}

QTabWidget::tab-bar {
    left: 5px; /* Indent to align with pane */
}

QTabBar::tab { /* General tab style */
    background-color: #FFFFFF; /* Light blue-grey inactive */
    color: #37474F; /* Darker blue-grey text for inactive tabs */
    border: 1px solid #BDBDBD;
    border-bottom: none;
    padding: 8px 15px;
    margin-right: 2px;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    min-width: 120px; /* Give tabs some minimum width */
}

QTabBar::tab:selected {
    background-color: #FFFFFF; /* Match pane background */
    color: #1E88E5; /* Brighter blue for selected text */
    border: 1px solid #BDBDBD;
    border-bottom: 1px solid #FFFFFF; /* Hide bottom border to merge */
    border-top: 3px solid #42A5F5; /* Accent blue top line */
    margin-bottom: -1px; /* Pull tab down slightly */
    font-weight: bold;
}

QTabBar::tab:!selected {
    margin-top: 2px; /* Push non-selected tabs down slightly */
}

QTabBar::tab:!selected:hover {
    background-color: #E0E0E0; /* Lighter grey on hover */
    color: #000000;
}

QGroupBox {
    background-color: #FFFFFF; /* White background */
    border: 1px solid #BDBDBD; /* Subtle border */
    border-radius: 4px;
    margin-top: 15px; /* Space for the title */
    padding: 10px;
    padding-top: 15px; /* More space inside top */
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    padding: 0 5px 0 5px;
    left: 10px; /* Indent title slightly */
    color: #546E7A; /* Blue-grey title text */
    background-color: #FFFFFF; /* Ensure title background matches groupbox */
    border: none;
    font-weight: bold;
}

QPushButton {
    background-color: #E0E0E0; /* Light grey button */
    color: #212121;
    border: 1px solid #BDBDBD;
    padding: 6px 15px; /* Slightly more padding */
    border-radius: 4px;
    min-height: 20px; /* Ensure consistent height */
    font-size: 10pt;
}

QPushButton:hover {
    background-color: #EEEEEE; /* Lighter grey on hover */
    border-color: #9E9E9E; /* Slightly darker border on hover */
}

QPushButton:pressed {
    background-color: #D5D5D5; /* Darker grey when pressed */
}

QPushButton:disabled {
    background-color: #F5F5F5;
    color: #9E9E9E;
    border-color: #E0E0E0;
}

QPushButton#btnUpdateCreateFileAuto, QPushButton#btnCreateFileAuto, QPushButton#btnUpload
{
   background-color: #42A5F5; /* Accent blue */
   color: white;
   border: none;
   font-weight: bold;
}

QPushButton#btnUpdateCreateFileAuto:hover, QPushButton#btnCreateFileAuto:hover, QPushButton#btnUpload:hover
{
   background-color: #1E88E5; /* Darker blue on hover */
}

QPushButton#btnUpdateCreateFileAuto:pressed, QPushButton#btnCreateFileAuto:pressed, QPushButton#btnUpload:pressed
{
   background-color: #0D47A1; /* Even darker blue */
}

QPushButton#btnUpdateCreateFileAuto:disabled, QPushButton#btnCreateFileAuto:disabled, QPushButton#btnUpload:disabled
{
   background-color: #90CAF9; /* Lighter blue when disabled */
   color: #E3F2FD;
   border: none;
}

QLineEdit, QComboBox {
    background-color: #FFFFFF;
    border: 1px solid #BDBDBD;
    padding: 4px 6px; /* Adjusted padding */
    border-radius: 4px;
    min-height: 22px; /* Adjusted min height */
    font-size: 10pt;
}

QLineEdit:focus, QComboBox:focus {
    border: 1px solid #42A5F5; /* Blue border on focus */
    /* background-color: #E3F2FD;  Optional: Light blue background on focus */
}

QLineEdit:read-only {
    background-color: #F5F5F5;
    color: #757575;
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px; /* Slightly wider dropdown button */
    border-left-width: 1px;
    border-left-color: #BDBDBD;
    border-left-style: solid;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
    background-color: #F5F5F5; /* Light grey background for dropdown arrow area */
}
QComboBox::down-arrow {
     image: url(:/qt-project.org/styles/commonstyle/images/downarraow-16.png); /* Default arrow - check path if needed */
     width: 10px;
     height: 10px;
}
QComboBox::down-arrow:on {
    top: 1px;
    left: 1px;
}

QComboBox:hover {
    border-color: #9E9E9E;
}

QComboBox QAbstractItemView {
    border: 1px solid #BDBDBD;
    background-color: #FFFFFF;
    selection-background-color: #BBDEFB;
    selection-color: #212121;
    padding: 2px;
}

QCheckBox {
    spacing: 8px;
    font-size: 10pt;
    background-color: transparent;
}
QCheckBox::indicator {
    width: 16px;
    height: 16px;
    border: 1px solid #757575;
    border-radius: 3px;
    background-color: #FFFFFF;
}
QCheckBox::indicator:hover {
     border-color: #42A5F5;
}
QCheckBox::indicator:checked {
    background-color: #42A5F5;
    border-color: #42A5F5;
    image: url(data:image/svg+xml;base64,); /* Embedded white check SVG */
}

QCheckBox::indicator:checked:hover {
    border-color: #1E88E5;
    background-color: #1E88E5;
}

QCheckBox:disabled {
    color: #9E9E9E;
}

QCheckBox::indicator:disabled {
    border-color: #E0E0E0;
    background-color: #F5F5F5;
}

QCheckBox::indicator:checked:disabled {
    background-color: #CFD8DC;
    border-color: #CFD8DC;
    image: url(data:image/svg+xml;base64,); /* Grey check */
}

QLabel {
    background-color: transparent;
    padding: 2px;
    font-size: 10pt;
}

QLabel#lblConnectionStatus {
    color: #388E3C;
    font-size: 24pt;
    font-weight: bold;
    qproperty-alignment: 'AlignCenter';
}

