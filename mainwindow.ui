QMainWindow {
    background-color: #ECEFF1;
}

QWidget {
    color: #212121;
    font-size: 10pt;
}

QTabWidget::pane {
    border: 1px solid #BDBDBD;
    background-color: #FFFFFF;
    margin-top: -1px;
    border-radius: 0 0 4px 4px;
}

QTabWidget::tab-bar {
    left: 5px;
}

QTabBar::tab {
    background-color: #FFFFFF;
    color: #37474F;
    border: 1px solid #BDBDBD;
    border-bottom: none;
    padding: 8px 15px;
    margin-right: 2px;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    min-width: 120px;
}

QTabBar::tab:selected {
    background-color: #FFFFFF;
    color: #1E88E5;
    border: 1px solid #BDBDBD;
    border-bottom: 1px solid #FFFFFF;
    border-top: 3px solid #42A5F5;
    margin-bottom: -1px;
    font-weight: bold;
}

QTabBar::tab:!selected {
    margin-top: 2px;
}

QTabBar::tab:!selected:hover {
    background-color: #E0E0E0;
    color: #000000;
}

QGroupBox {
    background-color: #FFFFFF;
    border: 1px solid #BDBDBD;
    border-radius: 4px;
    margin-top: 15px;
    padding: 10px;
    padding-top: 15px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    padding: 0 5px 0 5px;
    left: 10px;
    color: #546E7A;
    background-color: #FFFFFF;
    border: none;
    font-weight: bold;
}

QPushButton {
    background-color: #E0E0E0;
    color: #212121;
    border: 1px solid #BDBDBD;
    padding: 6px 15px;
    border-radius: 4px;
    min-height: 20px;
}

QPushButton:hover {
    background-color: #EEEEEE;
    border-color: #9E9E9E;
}

QPushButton:pressed {
    background-color: #D5D5D5;
}

QPushButton:disabled {
    background-color: #F5F5F5;
    color: #9E9E9E;
    border-color: #E0E0E0;
}

QPushButton#btnUpdateCreateFileAuto,
QPushButton#btnCreateFileAuto,
QPushButton#btnConnectAndUpload {
   background-color: #42A5F5;
   color: white;
   border: none;
   font-weight: bold;
}

QPushButton#btnUpdateCreateFileAuto:hover,
QPushButton#btnCreateFileAuto:hover,
QPushButton#btnConnectAndUpload:hover {
   background-color: #1E88E5;
}

QPushButton#btnUpdateCreateFileAuto:pressed,
QPushButton#btnCreateFileAuto:pressed,
QPushButton#btnConnectAndUpload:pressed {
   background-color: #0D47A1;
}

QPushButton#btnUpdateCreateFileAuto:disabled,
QPushButton#btnCreateFileAuto:disabled,
QPushButton#btnConnectAndUpload:disabled {
   background-color: #90CAF9;
   color: #E3F2FD;
   border: none;
}

QLineEdit, QComboBox {
    background-color: #FFFFFF;
    border: 1px solid #BDBDBD;
    padding: 4px 6px;
    border-radius: 4px;
    min-height: 22px;
}

QLineEdit:focus, QComboBox:focus {
    border: 1px solid #42A5F5;
}

QLineEdit:read-only {
    background-color: #F5F5F5;
    color: #757575;
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
    border-left-width: 1px;
    border-left-color: #BDBDBD;
    border-left-style: solid;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
    background-color: #F5F5F5;
}

QComboBox:hover {
    border-color: #9E9E9E;
}

QComboBox QAbstractItemView {
    border: 1px solid #BDBDBD;
    background-color: #FFFFFF;
    selection-background-color: #BBDEFB;
    selection-color: #212121;
    padding: 2px;
    outline: 0px;
}

QCheckBox {
    spacing: 8px;
    font-size: 10pt;
    background-color: transparent;
}

QCheckBox::indicator {
    width: 16px;
    height: 16px;
    border: 1px solid #757575;
    border-radius: 3px;
    background-color: #FFFFFF;
}

QCheckBox::indicator:hover {
     border-color: #42A5F5;
}

QCheckBox::indicator:checked {
    background-color: #42A5F5;
    border-color: #42A5F5;
}

QCheckBox::indicator:checked:hover {
    background-color: #1E88E5;
    border-color: #1E88E5;
}

QCheckBox:disabled {
    color: #9E9E9E;
}

QCheckBox::indicator:disabled {
    border-color: #E0E0E0;
    background-color: #F5F5F5;
}

QCheckBox::indicator:checked:disabled {
    background-color: #CFD8DC;
    border-color: #CFD8DC;
}

QLabel {
    background-color: transparent;
    padding: 2px;
    font-size: 10pt;
}

QLabel#lblConnectionStatus {
    font-size: 14pt;
}

